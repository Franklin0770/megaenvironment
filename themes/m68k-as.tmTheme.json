{
	"$schema": "vscode://schemas/color-theme",
	"name": "MegaEnvironment Dark",
	"tokenColors": [
		{
			"scope": "instruction.mnemonic.m68k-as",
			"settings": {
				"foreground": "#578dcb"
			}
		},
		{
			"scope": "instruction.size.m68k-as",
			"settings": {
				"foreground": "#457fdd"
			}
		},
		{
			"scope": "variable.register.m68k-as",
			"settings": {
				"foreground": "#6b68d3",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "constant.immediate.m68k-as",
			"settings": {
				"foreground": "#e0981c"
			}
		},
		{
			"scope": "constant.numeric.m68k-as",
			"settings": {
				"foreground": "#d3a345"
			}
		},
		{
			"scope": "directive.comment.m68k-as",
			"settings": {
				"foreground": "#ffffff70",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "directive.string.m68k-as",
			"settings": {
				"foreground": "#1fa03c"
			}
		},
		{
			"scope": "directive.label.global.m68k-as",
			"settings": {
				"foreground": "#d4cf37"
			}
		},
		{
			"scope": "directive.label.local.m68k-as",
			"settings": {
				"foreground": "#c8c44f"
			}
		},
		{
			"scope": "directive.label.temporary.m68k-as",
			"settings": {
				"foreground": "#c4c174"
			}
		},
		{
			"scope": "directive.label.nameless.m68k-as",
			"settings": {
				"foreground": "#FF6040"
			}
		},
		{
			"scope": "directive.operator.m68k-as",
			"settings": {
				"foreground": "#cb3567"
			}
		},
		{
			"scope": "directive.function.m68k-as",
			"settings": {
				"foreground": "#6fbdce"
			}
		},
		{
			"scope": "directive.symbol.m68k-as",
			"settings": {
				"foreground": "#c15bc1",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "directive.pseudo.m68k-as",
			"settings": {
				"foreground": "#9268B3"
			}
		}
	]
}