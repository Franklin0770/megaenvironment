{
	"$schema": "vscode://schemas/color-theme",
	"name": "MegaEnvironment Dark",
	"tokenColors": [
		{
			"scope": "instruction.mnemonic.m68k-as",
			"settings": {
				"foreground": "#578dcb"
			}
		},
		{
			"scope": "instruction.jump.m68k-as",
			"settings": {
				"foreground": "#578dcb",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "instruction.size.m68k-as",
			"settings": {
				"foreground": "#457fdd"
			}
		},
		{
			"scope": "instruction.autoincrement.m68k-as",
			"settings": {
				"foreground": "#DDDDDD"
			}
		},
		{
			"scope": "variable.register.m68k-as",
			"settings": {
				"foreground": "#6b68d3",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "constant.immediate.m68k-as",
			"settings": {
				"foreground": "#eca01c"
			}
		},
		{
			"scope": "constant.numeric.m68k-as",
			"settings": {
				"foreground": "#c29844"
			}
		},
		{
			"scope": "directive.comment.m68k-as",
			"settings": {
				"foreground": "#ffffff70",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "directive.string.m68k-as",
			"settings": {
				"foreground": "#1fa03c"
			}
		},
		{
			"scope": "directive.label.global.m68k-as",
			"settings": {
				"foreground": "#d6d143"
			}
		},
		{
			"scope": "directive.label.local.m68k-as",
			"settings": {
				"foreground": "#ccc969"
			}
		},
		{
			"scope": "directive.label.temporary.m68k-as",
			"settings": {
				"foreground": "#c0be82"
			}
		},
		{
			"scope": "directive.label.nameless.m68k-as",
			"settings": {
				"foreground": "#FF6040"
			}
		},
		{
			"scope": "directive.label.jump.m68k-as",
			"settings": {
				"foreground": "#b3ca88"
			}
		},
		{
			"scope": "directive.operator.m68k-as",
			"settings": {
				"foreground": "#ce557e"
			}
		},
		{
			"scope": "directive.function.m68k-as",
			"settings": {
				"foreground": "#6fbdce"
			}
		},
		{
			"scope": "directive.symbol.m68k-as",
			"settings": {
				"foreground": "#c15bc1",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "directive.token.m68k-as",
			"settings": {
				"foreground": "#c15bc1"
			}
		},
		{
			"scope": "directive.pseudo.m68k-as",
			"settings": {
				"foreground": "#9268B3"
			}
		}
	]
}