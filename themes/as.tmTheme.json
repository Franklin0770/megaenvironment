{
	"$schema": "vscode://schemas/color-theme",
	"name": "MegaEnvironment Dark",
	"tokenColors": [
		{
			"scope": "instruction.mnemonic.as",
			"settings": {
				"foreground": "#578dcb"
			}
		},
		{
			"scope": "instruction.jump.as",
			"settings": {
				"foreground": "#578dcb",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "instruction.size.as",
			"settings": {
				"foreground": "#457fdd"
			}
		},
		{
			"scope": "instruction.autoincrement.as",
			"settings": {
				"foreground": "#DDDDDD"
			}
		},
		{
			"scope": "variable.register.as",
			"settings": {
				"foreground": "#6b68d3",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "variable.flag.as",
			"settings": {
				"foreground": "#4899b9",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "constant.immediate.as",
			"settings": {
				"foreground": "#eca01c"
			}
		},
		{
			"scope": "constant.numeric.as",
			"settings": {
				"foreground": "#c68f21"
			}
		},
		{
			"scope": "directive.comment.as",
			"settings": {
				"foreground": "#ffffff70",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "directive.string.as",
			"settings": {
				"foreground": "#1fa03c"
			}
		},
		{
			"scope": "directive.label.global.as",
			"settings": {
				"foreground": "#d6d143"
			}
		},
		{
			"scope": "directive.label.local.as",
			"settings": {
				"foreground": "#ccc969"
			}
		},
		{
			"scope": "directive.label.temporary.as",
			"settings": {
				"foreground": "#c0be82"
			}
		},
		{
			"scope": "directive.label.nameless.as",
			"settings": {
				"foreground": "#FF6040"
			}
		},
		{
			"scope": "directive.label.jump.as",
			"settings": {
				"foreground": "#b5ce86"
			}
		},
		{
			"scope": "directive.operator.as",
			"settings": {
				"foreground": "#ce557e"
			}
		},
		{
			"scope": "directive.function.as",
			"settings": {
				"foreground": "#6fbdce"
			}
		},
		{
			"scope": "directive.symbol.as",
			"settings": {
				"foreground": "#c15bc1"
			}
		},
		{
			"scope": "directive.pseudo.as",
			"settings": {
				"foreground": "#9268B3"
			}
		}
	]
}